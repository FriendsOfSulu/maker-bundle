<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
<services>
    <defaults autowire="true" autoconfigure="true" />

    <service id="Mamazu\SuluMaker\ListConfiguration\XmlGenerator" />

    <service id="Mamazu\SuluMaker\ListConfiguration\MakeListConfigurationCommand">
        <argument>%kernel.project_dir%</argument>
        <argument type="service" id="Mamazu\SuluMaker\ListConfiguration\XmlGenerator" />
        <tag name="maker.command" />
    </service>
</services>
</container>
